version: "3"
services:
  postgres10:
    image: postgres:10.0-alpine
    restart: unless-stopped
    volumes:
      - "<%= @postgres10_data_path %>:/var/lib/postgresql/data"
    ports:
      - "30010:5432"
  redis3:
    image: redis:3.2.11-alpine
    restart: unless-stopped
    volumes:
      - "<%= @redis_data_path %>:/data"
    ports:
      - "30020:6379"
