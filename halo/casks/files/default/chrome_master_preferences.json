// See https://src.chromium.org/viewvc/chrome/trunk/src/chrome/common/pref_names.cc
{
  // Do not install any of the default extensions, such as Google Doc apps.
  "default_apps": "noinstall",

  // Show the regular "new tab" page instead of the homepage.
  "homepage_is_newtabpage": false,

  // When starting for the first time, open a blank page.
  "first_run_tabs": ["about:blank"],

  // New tabs open a blank page.
  "homepage": "about:blank",

  // Don't send anything typed into the search bar.
  "search": {
    "suggest_enabled": false,
    "contextual_search_enabled": false
  },

  // Increase privacy by disabling network predictions.
  "dns_prefetching": {
    "enabled": false
  },

  // Increase privacy by not using "a prediction service to load pages more quickly"
