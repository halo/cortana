  "message_center": {
    // Self Destroying Cookies is quite noisy
    "disabled_extension_ids": ["ghgghhceppfhemlgffnlbpaobcclhfdf"]
  },

  "bookmark_bar": {
    "show_on_all_tabs": false
  },

  "sync_promo": {
    "show_on_first_run_allowed": false
  },

  "autofill": {
    // Opting out of Google Wallet
    "pay_without_wallet": true,
    // Disable autofill
    "save_data": false
  },

  // Disable hyperlink auditing
  "enable_a_ping": false,

  // Deny Google services access to geolocation
  "googlegeolocationaccess": {
    "enabled": false
  },

  // I hope this does not break too many sites.
  "enable_referrers": false,

  // Why not.
  "enable_do_not_track": true,

  // Don't offer translation for web pages
  "translate": {
    "enabled": false
  },

  // Prevent "web service to help resolve navigation errors"
  "alternate_error_pages": {
    "enabled": false
  },

  "user_experience_metrics": {
    // Don't phone home to Google
    "reporting_enabled": false
  },

  "browser": {

    // Prevent any popups
    "has_seen_welcome_page": true,

    // This is just an annoying popup.
    "check_default_browser": false,

    // Hide the "Show home page" button in the menu bar.
    "show_home_button": false,

    // Don't ask for quitting while multiple tabs are stil open.
    "confirm_to_quit": false,

    // I suppose macOS has it's own spell checker?
    "enable_spellchecking": false,
    // Either way, don't mess with autocorrect, it never works.
    "enable_autospellcorrect": false,

    // Don't look for Google Cloud Printers in the LAN
    "local_discovery": {
      "notifications_enabled": false
    },

    // Don't unlock anything via Smartphones
    "easy_unlock": {
      "allowed": false
    },

    // Predefine checkboxes in clear history dialog
    "clear_data": {
      // Browsing data is the only thing we don't really need to clear.
      "browsing_history": false,
      "browsing_history_basic": false,
      // All the rest can go any time.
      "cache_basic": true,
      "cookies_basic": true,
      "cookies": true,
      "download_history": true,
      "form_data": true,
      "hosted_apps_data": true,
      "media_licenses": true,
      "passwords": true,
      "site_settings": true,
      // 4 is all time
      "time_period": 4,
      "time_period_basic": 4
    }
  },

  // Don't ask for any term agreement
  "EulaAccepted": true,

  "devtools": {
    // Don't look for USB devices for remote debugging at chrome://inspect
    "discover_usb_devices": false
  },

  "hardware": {
    // No Access to mic/cam
    "audio_capture_enabled": false,
    "video_capture_enabled": false
  },

  "hotword": {
    // Deactivate Voice Search
    "search_enabled_2": false,
    "always_on_search_enabled": false,
    "audio_logging_enabled": false
  },

  "background_mode": {
    // Don't create new login items
    "user_removed_login_item": true,
    "chrome_created_login_item": true
  },

  // No geolocation, ever.
  "googlegeolocationaccess": {
    "enabled": false
  },

  "profile": {

    "block_third_party_cookies": true,
    "clear_site_data_on_exit": true,

    "default_content_setting_values": {
      "background_sync": 2,
      "geolocation": 2,
      "media_stream_camera": 2,
      "media_stream_mic": 2,
      "notifications": 2
