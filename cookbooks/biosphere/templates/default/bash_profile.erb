# Load Homebrew binaries
export PATH="<%= @homebrew_bin_path %>:$PATH"

# Initialize rbenv
export RBENV_ROOT="<%= @rbenv_root_path %>"
hash <%= @rbenv_excutable %> && eval "$(<%= @rbenv_excutable %> init -)"

# Git fancy
if [[ -s <%= @homebrew_etc_path %>/bash_completion.d/git-prompt.sh ]]; then
  source <%= @homebrew_etc_path %>/bash_completion.d/git-prompt.sh
  export PS1='\w\$$(__git_ps1) '
fi
