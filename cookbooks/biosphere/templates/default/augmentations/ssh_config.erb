Host *
  ServerAliveInterval 5
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  SendEnv RMATE_*
  RemoteForward 52698 localhost:52698
  IdentityFile <%= @chef_key_path %>
  IdentitiesOnly yes
  User chef

Host <%= Github.host %>
  HostName github.com
  IdentityFile <%= @github_key_path %>
  User git
